{
  "name": "ai-cli-tool",
  "version": "0.1.0",
  "description": "AI-powered CLI assistant for developers",
  "type": "module",
  "main": "dist/cli/index.js",
  "bin": {
    "ai-cli": "dist/cli/index.js"
  },
  "scripts": {
    "dev": "tsx watch src/cli/index.ts",
    "build": "tsc",
